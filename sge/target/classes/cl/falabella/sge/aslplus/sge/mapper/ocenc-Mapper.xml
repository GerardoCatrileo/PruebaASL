<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cl.falabella.sge.aslplus.sge.dao.OcencMapper">  
    <select id = "numOC" resultType="cl.falabella.sge.aslplus.sge.entity.Ocenc">
        SELECT C_NEGOCIO, C_CANAL, C_OCENC
        FROM asl_ocenc
        WHERE c_negocio = 'F'
        AND c_canal = 'I'
        AND C_OCENC = '5000525041'
    </select>
</mapper>